<%- include("header", {title: title}); %>

<h1>Account details</h1>


<pre><%#= JSON.stringify(res1, null, 4) %></pre>

<table>
    <tr>
        <th>Id</th>
        <th>photo</th>
        <th>category</th>
        <th>type</th>
            <th>quantity</th>
        </tr>
<% for (const row of res) { %>
    <tr>
        <td><%= row.Id %></td>
        <td><%= row.photo %></td>
        <td><%= row.category %></td>
        <td><%= row.type %></td>
        <td><%= row.quantity %></td>
        <td>
            <a href="/product/edit/<%= row.id %>"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a>
            <a href="/product/delete/<%= row.id %>"><i class="fa fa-trash-o" aria-hidden="true"></i></a>
        </td>
    </tr>
<% }; %>
</table>

</main>