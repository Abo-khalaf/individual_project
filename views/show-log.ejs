<%- include("header"); %>
<body>
<h1 class="middle">Show Events</h1>

<form class="search-form" method="get" action="/eshop/log">
    <input class="search-style" type="text" name="search" placeholder="Search here ...">

    <input class="search-style-create" type="submit" value="search">
    </form>

<table>

    <tr>
        <th>Id</th>
        <th>photo</th>
        <th>category</th>
        <th>type</th>
            <th>quantity</th>
    </tr>
<% for (const row of res) { %>
    <tr>
        <td><%= row.Id %></td>
        <td><%= row.photo %></td>
        <td><%= row.category %></td>
        <td><%= row.type %></td>
        <td><%= row.quantity %></td>
    </tr>


<% }; %>

</table>



</body>
<%- include("../eshop/footer"); %>

